<Window x:Class="SnakeGameWPF.View.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeGameWPF.View"
        mc:Ignorable="d"
        Title="GameWindow" Height="450" Width="800">
    <Window.InputBindings>
        <KeyBinding Key="Up" Command="{Binding UpArrowKeyPressedCommand}"></KeyBinding>
        <KeyBinding Key="Down" Command="{Binding UpArrowKeyPressedCommand}"></KeyBinding>
        <KeyBinding Key="Right" Command="{Binding UpArrowKeyPressedCommand}"></KeyBinding>
        <KeyBinding Key="Left" Command="{Binding UpArrowKeyPressedCommand}"></KeyBinding>
    </Window.InputBindings>
    <Grid>
        
    </Grid>
</Window>
